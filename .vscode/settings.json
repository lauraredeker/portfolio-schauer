{
	//"css.customData": [".vscode/tailwind.json"],
	"[svelte]": {
		"editor.defaultFormatter": "svelte.svelte-vscode"
	},
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true
	},
	"hide-files.files": [],
	"editor.codeActionsOnSave": {
		"source.fixAll": "explicit"
	}
}
